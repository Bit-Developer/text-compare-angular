pipeline {
    agent any
    tools { nodejs "NodeJs" }
    stages {
        stage('Install & Build') {
            steps {
                sh 'npm install'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}